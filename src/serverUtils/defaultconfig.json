{
    "version": 1.0,
    "defaultLanguage": "cpp",
    "languages": {
        "c": {
            "isinterpreted": false,
            "compile": "gcc Main.c",
            "run": {
                "win32": "a.exe",
                "unix": "./a.out"
            },
            "template": "#include <stdio.h>\n"
        },
        "cpp": {
            "isinterpreted": false,
            "compile": "g++ Main.cpp",
            "run": {
                "win32": "a.exe",
                "unix": "./a.out"
            },
            "template": "#include <bits/stdc++.h>\n"
        },
        "python": {
            "isinterpreted": true,
            "run": {
                "win32": "python Main.py",
                "unix": "python3 Main.py"
            },
            "template": "t=int(input())"
        },
        "java": {
            "isinterpreted": false,
            "compile": "javac Main.java",
            "run": {
                "win32": "java Main.class",
                "unix": "java Main.class"
            },
            "template": "import java.utils.*;"
        }
    },
    "mountPoint": "."
}
